{
  "name": "font-awesome",
  "namespace": "Ext",
  "format": "1",
  "version": "6.0.1.250",
  "compatVersion": "4.3.0",
  "creator": "Sencha",
  "summary": "Package wrapper for Font Awesome",
  "detailedDescription": "Sencha Cmd package wrapper for Font Awesome (http://fortawesome.github.io/Font-Awesome/)",
  "type": "code",
  "resources": [
    {
      "path": "${package.dir}/resources",
      "output": "shared"
    }
  ],
  "framework": "ext",
  "toolkit": "classic",
  "theme": "theme-neptune",
  "output": "${framework.dir}/build/packages/${package.name}",
  "resource": {
    "paths": ""
  },
  "sass": {
    "namespace": "Ext",
    "etc": "${package.dir}/sass/etc/all.scss",
    "var": "${package.dir}/sass/var",
    "src": [
      "${package.dir}/sass/src",
      "${package.dir}/sass/src/all.scss"
    ]
  },
  "classpath": "${package.dir}/src",
  "overrides": "${package.dir}/overrides"
}